mixin admin()
  span.update
    a(href='[##_s_ad_m_link_##]') 수정
  span.delete
    a(href='#' onclick='[##_s_ad_d_onclick_##]') 삭제
  span.state
    a(href='#' onclick='[##_s_ad_s2_onclick_##]') [##_s_ad_s1_label_##]

span.admin
  if process.env.NODE_ENV === 'development'
    if process.env.IS_OWNER === 'true' && process.env.LOGIN === 'true'
      +admin()
  else
    s_ad_div
      +admin()
